{
  "version": "0.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@ant-design/icons": ">=4.7.0",
    "antd": ">=4.21.0",
    "lodash": ">=4.17.21",
    "react": ">=16"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "printWidth": 80,
    "endOfLine": "lf",
    "tabWidth": 2
  },
  "name": "@omnixenon/react-antd-json-schema-builder",
  "author": "Seun Adesina",
  "module": "dist/@omnixenon/react-antd-json-schema-builder.esm.js",
  "size-limit": [
    {
      "path": "dist/@omnixenon/react-antd-json-schema-builder.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/@omnixenon/react-antd-json-schema-builder.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/lodash": "^4.14.182",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "husky": "^8.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.21.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  }
}
